{
    "customerBaseDirectory":"/logs",
    "customerDefaultAttribute":{
        "type":"dateFile",
        "absolute":false,
        "alwaysIncludePattern":true
    },
    "appenders":[
        {"type":"console", "category":"console"},
        {"pattern":"debug/yyyyMMddhhmmss.txt", "category":"debug"},
        {"pattern":"info/yyyyMMddhhmmss.txt", "category":"info"},
        {"pattern":"warn/yyyyMMddhhmmss.txt", "category":"warn"},
        {"pattern":"error/yyyyMMddhhmmss.txt", "category":"error"}
    ],
    "replaceConsole":true,
    "levels":{"debug":"debug","info":"debug", "warn":"debug", "error":"debug"}
}
